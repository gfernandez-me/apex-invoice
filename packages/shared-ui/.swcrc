{
  "$schema": "https://swc.rs/schema.json",
  "exclude": ["node_modules/", "dist/", ".swc"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "esnext"
  },
  "module": {
    "type": "es6",
    "resolveFully": true
  }
}
